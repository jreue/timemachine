{
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "C_Cpp.default.compilerPath": "",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.cppStandard": "c++11",
  "C_Cpp.default.defines": [
    "PLATFORMIO=60111",
    "ARDUINO_AVR_UNO",
    "F_CPU=16000000L",
    "ARDUINO_ARCH_AVR",
    "ARDUINO=10808",
    "__AVR_ATmega328P__"
  ],
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/include",
    "${workspaceFolder}/src",
    "${workspaceFolder}/lib",
    "${workspaceFolder}/.pio/libdeps/uno",
    "~/.platformio/packages/framework-arduino-avr/libraries",
    "~/.platformio/packages/framework-arduino-avr/cores/arduino",
    "~/.platformio/packages/toolchain-atmelavr/avr/include"
  ],
  "files.associations": {
    "*.ino": "cpp",
    "arduino.h": "c",
    "string.h": "c",
    "stdio.h": "c",
    "wstring.h": "c"
  },
  "editor.formatOnSave": true,
  "platformio-ide.useBuiltinPIOCore": true,
  "platformio-ide.activateProjectOnTextEditorOpened": true,
  "platformio-ide.autoRebuildAutocompleteIndex": true
}
